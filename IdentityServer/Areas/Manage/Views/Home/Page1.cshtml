@{
    Layout = "_IFrameLayout";
    ViewData["Title"] = "Page1";
}
@section Styles{ 
<style>
    body {
        background-color: #f3f3f4;
    }
    </style>
}
    <div class="wrapper wrapper-content">
        <div class="row">
            <div class="col-sm-6">
                <div class="ibox float-e-margins">
                    <div class="ibox-title">
                        <span class="label label-success pull-right">月</span>
                        <h5>收入</h5>
                    </div>
                    <div class="ibox-content">
                        <h1 class="no-margins">40 886,200</h1>
                        <div class="stat-percent font-bold text-success">
                            98% <i class="fa fa-bolt"></i>
                        </div>
                        <small>总收入</small>
                    </div>
                </div>
            </div>
            <div class="col-sm-6">
                <div class="ibox float-e-margins">
                    <div class="ibox-title">
                        <span class="label label-info pull-right">全年</span>
                        <h5>订单</h5>
                    </div>
                    <div class="ibox-content">
                        <h1 class="no-margins">275,800</h1>
                        <div class="stat-percent font-bold text-info">
                            20% <i class="fa fa-level-up"></i>
                        </div>
                        <small>新订单</small>
                    </div>
                </div>
            </div>
        </div>
    </div>



@section Scripts
    {


    <script>
        layui.use(['layer'], function () {
        });
    </script>
    <!-- 全局js -->
    <!-- Flot -->
    <script src="~/library/h-plus/js/plugins/flot/jquery.flot.js"></script>
    <script src="~/library/h-plus/js/plugins/flot/jquery.flot.tooltip.min.js"></script>
    <script src="~/library/h-plus/js/plugins/flot/jquery.flot.spline.js"></script>
    <script src="~/library/h-plus/js/plugins/flot/jquery.flot.resize.js"></script>
    <script src="~/library/h-plus/js/plugins/flot/jquery.flot.pie.js"></script>
    <script src="~/library/h-plus/js/plugins/flot/jquery.flot.symbol.js"></script>
    <!-- Peity -->
    <script src="~/library/h-plus/js/plugins/peity/jquery.peity.min.js"></script>
    <script src="~/library/h-plus/js/demo/peity-demo.js"></script>
    <!-- 自定义js -->
    <script src="~/library/h-plus/js/content.js"></script>
    <!-- jQuery UI -->
    <script src="~/bower/jquery-ui/jquery-ui.js"></script>
    <!-- Jvectormap -->
    <script src="~/library/h-plus/js/plugins/jvectormap/jquery-jvectormap-1.2.2.min.js"></script>
    <script src="~/library/h-plus/js/plugins/jvectormap/jquery-jvectormap-world-mill-en.js"></script>
    <!-- EayPIE -->
    <script src="~/library/h-plus/js/plugins/easypiechart/jquery.easypiechart.js"></script>
    <!-- Sparkline -->
    <script src="~/library/h-plus/js/plugins/sparkline/jquery.sparkline.min.js"></script>
    <!-- Sparkline demo data  -->
    <script src="~/library/h-plus/js/demo/sparkline-demo.js"></script>
    <!--统计代码，可删除--> 
}



